import java.io.*;
import java.util.*;

public class ListDir2 {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		String f = scanner.next();
		File file = new File(f);
		
		if(!file.isDirectory()) {
			System.out.println(f + " is not a directory.");
			System.exit(0);
		}
		
		StringBuilder sb = new StringBuilder(256);
		String[] contents = file.list();
		for(int i = 0 ; i < contents.length ; i++)
			sb.append(contents[i]).append("\n");
		System.out.println(sb);
		
		for(int i = 0 ; i < contents.length ; i++)
			System.out.println(file.listFiles()[i]);
		
		StringBuilder sb2 = new StringBuilder(256);
		for(int i = 0 ; i < file.listFiles().length ; i++) {
			if(file.listFiles()[i].isFile()) sb2.append("file:"); 
			if(file.listFiles()[i].isDirectory()) sb2.append("dir:");
			sb.append(file.list()[i]).append("\n");
		}
				
		scanner.close();

	}

}
